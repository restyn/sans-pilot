<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_picking_form_attendees" model="ir.ui.view">
        <field name="name">stock.picking.form.attendees</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.view_picking_form"/>
        <field name="arch" type="xml">
              <xpath expr="//form[1]/sheet[1]/notebook[1]" position="inside">
                <page name="studio_page_xRe5N" string="Attendees">
                  <group name="studio_group_xRe5N">
                    <field name="attendees">
                      <tree string="Registration">
                        <field name="event_ticket_id"/>
                        <field name="state"/>
                        <field name="street"/>
                        <field name="city"/>
                        <field name="state_id"/>
                        <field name="zip"/>
                      </tree>
                    </field>
                  </group>
                </page>
              </xpath>
        </field>
    </record>
</odoo>